<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddemployeeController</servlet-name>
  	<display-name>AddemployeeController</display-name>
  	<description></description>
  	<servlet-class>com.assignment.web.AddemployeeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetDepartments</servlet-name>
  	<display-name>GetDepartments</display-name>
  	<description></description>
  	<servlet-class>com.assignment.web.GetDepartments</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetEmployeeController</servlet-name>
  	<display-name>GetEmployeeController</display-name>
  	<description></description>
  	<servlet-class>com.assignment.web.GetEmployeeController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddemployeeController</servlet-name>
  	<url-pattern>/Addemployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetDepartments</servlet-name>
  	<url-pattern>/GetDepartments</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetEmployeeController</servlet-name>
  	<url-pattern>/GetEmployeeController</url-pattern>
  </servlet-mapping>
</web-app>
